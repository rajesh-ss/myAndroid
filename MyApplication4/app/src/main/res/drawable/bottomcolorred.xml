<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="2dp"
                android:color="#23F15050" />
            <solid android:color="#19F15050" />

        </shape>
    </item>

    <item android:top="0dp" android:bottom="1dp" android:right="0dp" android:left="0dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="#F60707" />
            <solid android:color="#C9F15050" />
        </shape>
    </item>

</layer-list>
